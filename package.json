{
  "name": "homepage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.vue src"
  },
  "dependencies": {
    "aplayer": "^1.10.1",
    "axios": "^1.4.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@types/node": "^18.16.18",
    "@vicons/fa": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.4",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.2"
  },
  "eslintConfig": {
    "extends": "@antfu",
    "rules": {
      "vue/component-tags-order": [
        "error",
        {
          "order": [
            "template",
            "script",
            "style"
          ]
        }
      ]
    }
  }
}
